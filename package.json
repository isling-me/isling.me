{
  "name": "isling-blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "0.1.0-beta.10",
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "apollo-boost": "^0.4.3",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-beta.2",
    "graphql": "^14.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "react-medium-editor": "^1.8.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/assets/css/index.css",
    "watch:css": "postcss src/styles/index.css -o src/assets/css/index.css -w",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "gcp:push": "gcloud compute scp ./build/* isling:~/isling.me --recurse",
    "deploy": "npm run build && npm run gcp:push",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "postcss-cli": "^6.1.2",
    "tailwindcss": "^1.0.3"
  }
}
