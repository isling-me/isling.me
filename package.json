{
  "name": "tailwind",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/assets/css/index.css",
    "watch:css": "postcss src/styles/index.css -o src/assets/css/index.css --watch",
    "start": "react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "gcp:push": "gcloud compute scp ./build/* isling:~/isling.me --recurse",
    "deploy": "npm run build && npm run gcp:push",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "postcss-cli": "^6.1.2",
    "tailwindcss": "^1.0.3"
  }
}
